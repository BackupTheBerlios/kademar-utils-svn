#Restore original QT Theme on the end
if [ -e /tmp/qt-theme-orig ]; then
  . /tmp/qt-theme-orig
  sed "s~style=.*~style=$style~g" -i $HOME/.config/Trolltech.conf
fi


. /etc/kademar/functions
set_environment_variables

if [ -d /etc/X11/xinit/xinitrc.d ]; then
  for f in /etc/X11/xinit/xinitrc.d/*; do
    [ -x "$f" ] && . "$f"
  done
  unset f
fi

#exec ck-launch-session dbus-launch startlxdeaccessibility

#Support for both types

if [ -e /usr/bin/startlxdeaccessibility ]; then
    exec startlxdeaccessibility
else
    exec startlxde
fi