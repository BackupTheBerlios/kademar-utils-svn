#Hack per engegar el CADI
#Xorg 7.3 - Composite  Support
echo "[Desktop Entry]
Comment=
Comment[es]=
Exec=sudo cadi
GenericName=
GenericName[es]=
Icon=package_settings
MimeType=
Name=CADI
Name[es]=CADI
Path=
StartupNotify=true
Terminal=false
TerminalOptions=
Type=Application
X-DCOP-ServiceType=
X-KDE-SubstituteUID=false
X-KDE-Username=" > /home/$user/.kde3/share/apps/kicker/CADI.desktop

