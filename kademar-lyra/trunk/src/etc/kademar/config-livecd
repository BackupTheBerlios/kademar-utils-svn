#Configuration Live-CD file. Used Only when is in live-cd mode

#Kind (default: lyra)
kademar_type="Lyra"

#Image to use by Installer, to fill kdmrc config file
kdm_image="/usr/share/wallpapers/kademar.png"

#background to use on xsession live-cd start (not desktop wallpaper)
#  MUST BE A  PNG  FILE
#for normal monitor
xsession_background="/usr/share/wallpapers/kademar.png"
#for panoramic monitor
xsession_background_panoramic="/usr/share/wallpapers/kademar-panoramic.png"


#Minimum Desktop
desktop_min=icewm

#Maximum Desktop
desktop_max=kde

#Detecta velocitat per definir desktop
detecta_desktop="si"