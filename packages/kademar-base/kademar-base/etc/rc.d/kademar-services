#!/bin/bash

#mount all
mnt-kademar

#Start VirtualBox services if it's virtual session
if [ -n "$(lspci | grep -i virtualbox)" ]; then
 modprobe -a vboxguest vboxsf vboxvideo
 mount -t vboxsf VirtualBox_SharedFolder /media/VirtualBox_SharedFolder -o gid=group,rw,dmode=777,fmode=777,comment=systemd.automount
fi

#Process events that we couldn't earlier (udev without dbus started)
udevadm trigger --action=add

